    $oldVenueID = $_POST["old_venueID"];

    // Retrieve new data from POST variables
    $newStartTimeArray = $_POST["start_time"];
    $newEndTimeArray = $_POST["end_time"];
    $newDayArray = $_POST["day"];
    $newClassTypeArray = $_POST["classtype"];
    $newVenueIDArray = $_POST["venueID"];

    // Debugging: Print the contents of $_POST
    var_dump($_POST);

    // Perform any necessary processing with the old and new data
    // For example, you could update the database with the new data

    // Redirect back to the form with a success message
    header("Location: requestform.php?success=true");
